<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-lazy-init="true"
	default-dependency-check="none" default-merge="false">
	<bean id="nameManager" class="com.ucard.service.NameManager"
		lazy-init="default" autowire="default" dependency-check="default" />
	<bean id="rightService" class="com.ucard.service.RightService"
		lazy-init="default" autowire="default" dependency-check="default" />
	<bean id="pushManager" class="com.ucard.framework.push.impl.JPush"
		lazy-init="default" autowire="default" dependency-check="default" />			
</beans>